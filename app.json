{
  "expo": {
    "name": "Simple",
    "slug": "simplefitnessappbeta",
    "version": "1.7.8",
    "orientation": "portrait",
    "icon": "./assets/simpleicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/simpleicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "tr.com.basarsubasi.simplefitnessapp"
    },
    "android": {
      "versionCode": 50,
      "blockedPermissions": ["android.permission.RECORD_AUDIO"],      
      "adaptiveIcon": {
        "foregroundImage": "./assets/simpleicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "tr.com.basarsubasi.simplefitnessapp"
    },
    "web": {
      "favicon": "./assets/simpleicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1b72cc6b-8e1a-47b1-a7d3-8fcf199a3015"
      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-build-properties",
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationicon.png",
          "color": "#000000",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "owner": "basarsubasi"
  }
}
