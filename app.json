{
  "expo": {
    "name": "Simple",
    "slug": "simpleios",
    "version": "1.7.13",
    "orientation": "portrait",
    "icon": "./assets/simpleicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/simpleicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet" : false,
      "bundleIdentifier": "com.basarsubasi.simplefitnessapp",
      "buildNumber": "1.6.7"
    },
    "android": {
      "versionCode":53,
      "adaptiveIcon": {
        "foregroundImage": "./assets/simpleicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "tr.com.basarsubasi.simplefitnessapp"
    },
    "web": {
      "favicon": "./assets/simpleicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "304669cf-e466-4f48-8212-17d769256e30"      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-build-properties",
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationicon.png",
          "color": "#000000",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "owner": "basarsubasi"
  }
}
