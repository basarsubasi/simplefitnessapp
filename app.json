        "foregroundImage": "./assets/simpleicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "tr.com.basarsubasi.simplefitnessapp"
    },
    "web": {
      "favicon": "./assets/simpleicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "304669cf-e466-4f48-8212-17d769256e30"
      }
    },
    "plugins": [
      "expo-sqlite"
    ],
    "owner": "basarsubasi"
  }
}
